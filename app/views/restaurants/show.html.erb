<h2>Restaurant Details</h2>
<p><%= @restaurant.name %></p>
<p><%= @restaurant.address %></p>
<p><%= @restaurant.phone_number %></p>
<p><%= @restaurant.category %></p>
<% @reviews.each do |r| %>
<div>
<%= r.content %>
<%= r.rating %>
</div>
<% end %>
<%= link_to "Add review", new_restaurant_review_path(@restaurant.id), class: "btn btn-primary"%>
